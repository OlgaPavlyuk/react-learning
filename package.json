{
  "name": "react-learning",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://OlgaPavlyuk.github.io/react-learning",
  "dependencies": {
    "classnames": "^2.2.6",
    "eslint-config-airbnb-base": "^14.0.0",
    "lodash": "^4.17.15",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-transition-group": "^1.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "react-app"
    ],
    "rules": {
      "no-console": 0,
      "linebreak-style": 0,
      "object-curly-newline": [
        "error",
        {
          "ImportDeclaration": "never",
          "ExportDeclaration": "never"
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^2.1.1"
  }
}
